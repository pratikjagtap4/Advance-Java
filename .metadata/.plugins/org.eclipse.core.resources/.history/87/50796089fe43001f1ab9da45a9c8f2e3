package lambdaexp;

class EmployeeData {
	int empId;
	String empName;
	String address;
	String job;
	int salary;
	
	public EmployeeData(int empId, String empName, String address, String job, int salary) {
		this.empId = empId;
		this.empName = empName;
		this.address = address;
		this.job = job;
		this.salary = salary;
	}	
}
public class Ex14StreamApiObj {

	public static void main(String[] args) {
		EmployeeData emp1 = new EmployeeData(101 , "Pratik Jagtap" , "Kondhwa" ,"Developer" , 35000);
	}

}
