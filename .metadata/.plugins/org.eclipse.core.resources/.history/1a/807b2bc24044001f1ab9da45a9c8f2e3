package basics;


class BankDetails 
{
	int balance = 10000;
	
	public void withdraw(int amount)
	{
		if(balance < amount)
		{
			System.out.println("Insufficient balance");
		}
		balance -=balance;
		System.out.println("Your balanace is " + balance);
	}
}
