package lambdaexp;

import java.util.ArrayList;

public class Ex15StreamApiTerminal {

	public static void main(String[] args) {
		ArrayList<Integer> nums = new ArrayList();
		
		nums.add(1);
		nums.add(2);
		nums.add(3);
		nums.add(4);
		nums.add(5);
		nums.add(6);
	
		
		int fac = nums.stream().reduce(1, (fac , n) ->fac*n);
	}

}
