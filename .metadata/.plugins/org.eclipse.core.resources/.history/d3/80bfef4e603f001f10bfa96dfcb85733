package lambdaexp;

public class Ex2CarBrands {

	public static void main(String[] args) {
		
		Ex2Cars car1 =  (name , price) -> {return name;} ;
		System.out.println(car1.details("Thar", 1700000));
		
		Ex2Cars car2 = (name , price) -> name;
		System.out.println(car2.details("Onmi" , 200000));
		
		Ex2Cars car3 = (name , price) -> name;
		System.out.println(car3.details("nano" , 100000));
	}	

}
