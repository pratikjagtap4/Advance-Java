package lambdaexp;

class EmployeeData {
	int empId;
	String empName;
	String address;
	String job;
	int salary;
	
	public EmployeeData(int empId, String empName, String address, String job, int salary) {
		this.empId = empId;
		this.empName = empName;
		this.address = address;
		this.job = job;
		this.salary = salary;
	}	
}
public class Ex14StreamApiObj {

	public static void main(String[] args) {
		EmployeeData emp1 = new EmployeeData(101 , "Pratik Jagtap" , "Kondhwa" ,"Developer" , 55000);
		EmployeeData emp2 = new EmployeeData(202 , "Dhananjay Zurange" , "Kondhwa" ,"Animation" , 35000);
		EmployeeData emp3 = new EmployeeData(303 , "Tushar Jambhulkar" , "Kothrud" ,"Tester" , 40000);
		EmployeeData emp4 = new EmployeeData(404 , "Lucky Kore" , "Solapur" ,"Developer" , 37000);
		EmployeeData emp5 = new EmployeeData(505 , "Hitesh Choudhary" , "Jaipur" ,"Trainer" , 85000);
		EmployeeData emp6 = new EmployeeData(606 , "Ritesh Naidu" , "Kolhapur" ,"Trainer" , 115000);
		EmployeeData emp7 = new EmployeeData(707 , "Sidhart Sirohia" , "Gangtok" ,"Trainer" , 75000);
	}

}
