package basics;


class PrintTask implements Runnable
{
	char printChar;
	
	PrintTask(char printChar)
	{
		this.printChar = printChar;
	}
	
	public void run() {
		for(int i = 1 ; i<= 100 ; i++)
		{
			System.out.println(i+printChar);
		}
		
	}
	
}
public class Ex19ExecutorService {

	public static void main(String[] args) {
		

	}

}
